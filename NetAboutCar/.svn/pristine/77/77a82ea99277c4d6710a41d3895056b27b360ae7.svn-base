package com.nasoft.aboutcar.car.domain;


/**
 * 车辆营运许可审批信息
 * @author 周鹏飞
 *
 */
public class CarApprove {

	private String appRid;
	private String applyId;
	private String apprType;
	private String opinions;
	private String description;
	private String apprUserId;
	private String apprInstId;
	private String apprDateTime;
	public String getAppRid() {
		return appRid;
	}
	public void setAppRid(String appRid) {
		this.appRid = appRid;
	}
	public String getApplyId() {
		return applyId;
	}
	public void setApplyId(String applyId) {
		this.applyId = applyId;
	}
	public String getApprType() {
		return apprType;
	}
	public void setApprType(String apprType) {
		this.apprType = apprType;
	}
	public String getOpinions() {
		return opinions;
	}
	public void setOpinions(String opinions) {
		this.opinions = opinions;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getApprUserId() {
		return apprUserId;
	}
	public void setApprUserId(String apprUserId) {
		this.apprUserId = apprUserId;
	}
	public String getApprInstId() {
		return apprInstId;
	}
	public void setApprInstId(String apprInstId) {
		this.apprInstId = apprInstId;
	}
	public String getApprDateTime() {
		return apprDateTime;
	}
	public void setApprDateTime(String apprDateTime) {
		this.apprDateTime = apprDateTime;
	}
}
