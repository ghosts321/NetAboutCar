@CHARSET"UTF-8";
.info-statistics{
	    font-size: 120%;
    font-weight: bold;
}

 #fontStyle{
 	size:20px;
 	padding-left:20px
 }
 
 .row-col{
 	float:left;
 }
 
 .search-row,.button-row{
    display: block;
    width: 100%;
    min-height: 30px;
    line-height:30px;
    margin-bottom: 15px;
 }
 
 .search-options{
 	
 }
 
 .search-options ul{
 	 list-style:none;
 }
 
 .search-options ul li{
 	float:left;
 	height:100%;
 }
 
 .search-options ul li .search-option{
 	padding: 3px 6px;
    border: 1px solid #efe9e9;
    margin-right: 10px;
    cursor:hand;
    color:#756a6a;
 }
 
 
 .search-options ul li .search-option-select{
 	color:blue;
 	border: 1px solid #6782bd;
 	background-color: #f8f8fb;
 }
 
  .search-options ul li .search-option:hover{
 	text-decoration: none;
 	color:blue;
 	border: 1px solid #6782bd;
 }
 
 .row-title{
     width: 80px;
 }
 
 .charts{
 	width:100%;
 	height:600px;
    border: 1px solid #a7a1a1;
    padding: 10px
 }
 
 .chart{
 	widht:99%;
 	height:99%;
 }
 
 
 .btn-info{
	float:right;
 }