package com.nasoft.aboutcar.drvuserinfo.domain;

import java.util.Date;

/**
 * 用户表（申请）
 * @author user
 *
 */


public class DrvUserreg { 
	private String certno;//身份证号码
	private String password;//密码
	private String phoneno;//注册手机号
	private Date regdatetime;//注册时间
	private Date updtime;//修改时间
	private String status;//账号状态
	private Integer pwderrtimes;//密码输入错误次数
	private Date lockdatetime;//账户锁定时间
	private Date smssendtime;//短信发送时间
	private Integer smssendcount;//短信发送次数
	private Date smsfirsttime;//短信第一次发送时间
	private Integer smserrcount;//短信输入错误次数
	private Date smslocktime;//短信锁定时间
	
	private String areaname;  //区域名称
	public String getCertno() {
		return certno;
	}
	public void setCertno(String certno) {
		this.certno = certno;
	}
	public Date getRegdatetime() {
		return regdatetime;
	}
	public void setRegdatetime(Date regdatetime) {
		this.regdatetime = regdatetime;
	}
	public Date getLockdatetime() {
		return lockdatetime;
	}
	public void setLockdatetime(Date lockdatetime) {
		this.lockdatetime = lockdatetime;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getPhoneno() {
		return phoneno;
	}
	public void setPhoneno(String phoneno) {
		this.phoneno = phoneno;
	}
	public Date getUpdtime() {
		return updtime;
	}
	public void setUpdtime(Date updtime) {
		this.updtime = updtime;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Integer getPwderrtimes() {
		return pwderrtimes;
	}
	public void setPwderrtimes(Integer pwderrtimes) {
		this.pwderrtimes = pwderrtimes;
	}
	public Date getSmssendtime() {
		return smssendtime;
	}
	public void setSmssendtime(Date smssendtime) {
		this.smssendtime = smssendtime;
	}
	public Integer getSmssendcount() {
		return smssendcount;
	}
	public void setSmssendcount(Integer smssendcount) {
		this.smssendcount = smssendcount;
	}
	public Date getSmsfirsttime() {
		return smsfirsttime;
	}
	public void setSmsfirsttime(Date smsfirsttime) {
		this.smsfirsttime = smsfirsttime;
	}
	public Integer getSmserrcount() {
		return smserrcount;
	}
	public void setSmserrcount(Integer smserrcount) {
		this.smserrcount = smserrcount;
	}
	public Date getSmslocktime() {
		return smslocktime;
	}
	public void setSmslocktime(Date smslocktime) {
		this.smslocktime = smslocktime;
	}
	public String getAreaname() {
		return areaname;
	}
	public void setAreaname(String areaname) {
		this.areaname = areaname;
	}
	
}

