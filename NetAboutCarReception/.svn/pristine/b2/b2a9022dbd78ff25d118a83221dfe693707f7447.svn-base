package com.nasoft.SSM.domain;

import java.io.Serializable;

public class DrvCaseDealInfo implements Serializable{
	

	private String caseId;       //案件编码
	private int dealUserId;   //承办人Id
	private int lastDealUserId;   //上级承办人Id
	private int nextDealUserId;   //下级承办人Id
	private String dealStatus;   //处罚状态  00 受理   01 处理中  02 处罚告知   03申诉   04 结案
	private String punishType;  //处罚类型  0001 口头警告   0002 两百元一下罚款   0003暂扣或吊销驾驶证
	private String dealView;  //处理意见
	private String evidenceView;   //取证意见
	private int createUserId;//创建人
	private String createInstId;//创建机构
	private String createTime;//创建时间
	private String updUserId;//修改人
	private String updInstId;//修改机构
	private String updateTime;//修改时间
	private String delFlag;//删除标识  00默认 01删除
	private String endUpdateTime;  //最后更新时间
	public String getCaseId() {
		return caseId;
	}
	public void setCaseId(String caseId) {
		this.caseId = caseId;
	}
	public int getDealUserId() {
		return dealUserId;
	}
	public void setDealUserId(int dealUserId) {
		this.dealUserId = dealUserId;
	}
	public int getLastDealUserId() {
		return lastDealUserId;
	}
	public void setLastDealUserId(int lastDealUserId) {
		this.lastDealUserId = lastDealUserId;
	}
	public int getNextDealUserId() {
		return nextDealUserId;
	}
	public void setNextDealUserId(int nextDealUserId) {
		this.nextDealUserId = nextDealUserId;
	}
	
	public String getDealStatus() {
		return dealStatus;
	}
	public void setDealStatus(String dealStatus) {
		this.dealStatus = dealStatus;
	}
	public String getPunishType() {
		return punishType;
	}
	public void setPunishType(String punishType) {
		this.punishType = punishType;
	}
	public String getDealView() {
		return dealView;
	}
	public void setDealView(String dealView) {
		this.dealView = dealView;
	}
	public String getEvidenceView() {
		return evidenceView;
	}
	public void setEvidenceView(String evidenceView) {
		this.evidenceView = evidenceView;
	}
	public int getCreateUserId() {
		return createUserId;
	}
	public void setCreateUserId(int createUserId) {
		this.createUserId = createUserId;
	}
	public String getCreateInstId() {
		return createInstId;
	}
	public void setCreateInstId(String createInstId) {
		this.createInstId = createInstId;
	}
	public String getCreateTime() {
		return createTime;
	}
	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}
	public String getUpdUserId() {
		return updUserId;
	}
	public void setUpdUserId(String updUserId) {
		this.updUserId = updUserId;
	}
	public String getUpdInstId() {
		return updInstId;
	}
	public void setUpdInstId(String updInstId) {
		this.updInstId = updInstId;
	}
	public String getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}
	public String getDelFlag() {
		return delFlag;
	}
	public void setDelFlag(String delFlag) {
		this.delFlag = delFlag;
	}
	public String getEndUpdateTime() {
		return endUpdateTime;
	}
	public void setEndUpdateTime(String endUpdateTime) {
		this.endUpdateTime = endUpdateTime;
	}
	
	
	
	
}
