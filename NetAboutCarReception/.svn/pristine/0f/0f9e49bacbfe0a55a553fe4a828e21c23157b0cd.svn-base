package com.nasoft.SSM.domain;

import java.io.Serializable;

public class comComplainAppeal implements Serializable {

	private int complain_appeal_id;//申诉ID
	private int complain_info_id;//投诉信息id
	private String appeal_file;//申诉文件
	private String appl_content;//申诉内容
	private String appl_peo;//申诉人
	private String appl_time;//申诉时间
	private String appl_dis_idea;//申诉受理意见
	private String appl_dis_result;//申诉受理结果
	private String appl_dis_peo;//申诉受理人
	private String appl_dis_time;//申诉受理时间
	public int getComplain_appeal_id() {
		return complain_appeal_id;
	}
	public void setComplain_appeal_id(int complain_appeal_id) {
		this.complain_appeal_id = complain_appeal_id;
	}
	public int getComplain_info_id() {
		return complain_info_id;
	}
	public void setComplain_info_id(int complain_info_id) {
		this.complain_info_id = complain_info_id;
	}
	public String getAppeal_file() {
		return appeal_file;
	}
	public void setAppeal_file(String appeal_file) {
		this.appeal_file = appeal_file;
	}
	public String getAppl_content() {
		return appl_content;
	}
	public void setAppl_content(String appl_content) {
		this.appl_content = appl_content;
	}
	public String getAppl_peo() {
		return appl_peo;
	}
	public void setAppl_peo(String appl_peo) {
		this.appl_peo = appl_peo;
	}
	public String getAppl_time() {
		return appl_time;
	}
	public void setAppl_time(String appl_time) {
		this.appl_time = appl_time;
	}
	public String getAppl_dis_idea() {
		return appl_dis_idea;
	}
	public void setAppl_dis_idea(String appl_dis_idea) {
		this.appl_dis_idea = appl_dis_idea;
	}
	public String getAppl_dis_result() {
		return appl_dis_result;
	}
	public void setAppl_dis_result(String appl_dis_result) {
		this.appl_dis_result = appl_dis_result;
	}
	public String getAppl_dis_peo() {
		return appl_dis_peo;
	}
	public void setAppl_dis_peo(String appl_dis_peo) {
		this.appl_dis_peo = appl_dis_peo;
	}
	public String getAppl_dis_time() {
		return appl_dis_time;
	}
	public void setAppl_dis_time(String appl_dis_time) {
		this.appl_dis_time = appl_dis_time;
	}
	
	
	
}
