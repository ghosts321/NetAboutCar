@charset "utf-8";
/* CSS Document */

.buss_area {
}

.buss_area:after {
	content: "."; 
    display: block;
    height: 0; 
    clear: both; 
    visibility: hidden;
}

.buss_area .personalmenu {
	width: 220px;
	min-width: 220px;
	height: auto;
	min-height: 500px;
	background-color: #ffffff;
	border: 1px solid #c9c9c9;
	padding: 0px;
	margin: 0px auto;
	float: left;
}

.buss_area .workarea {
	float: right;
	width: 746px;
	padding: 10px;
	margin-left: 10px;
	border: 1px solid #c9c9c9;
}

.buss_area .workarea .title_area {
	text-align: center;
	margin-bottom: 20px;
}

.buss_area .workarea .title_area:after {
	content: "."; 
    display: block;
    height: 0; 
    clear: both; 
    visibility: hidden;
}

.buss_area .workarea .title_area .title {
	height: 30px;
	font-size: 18px;
	color: #9370DB;
	text-align: center;
	padding: 10px;
	border-bottom: 1px solid #9370DB;
	position: relative;
}

.buss_area .workarea .title_area .help {
	line-height: 20px;
	font-size: 14px;
	color: #626262;
	float:right;
	position: absolute;
	right: 5px;
	top: 20px;
}

.buss_area .workarea .title_area .help .guide {
	display: inline-block;
	line-height: 20px;
	color: #a0a0a0;
	padding-left: 22px;
	background: url(../images/bussdeal/guide.png) no-repeat 0px -20px;
}

.buss_area .workarea .title_area .help .guide:hover {
	color: #626262;
	background-position: 0px 0px;
}

.buss_area .workarea .title_area .help .policy {
	display: inline-block;
	line-height: 20px;
	color: #a0a0a0;
	padding-left: 22px;
	margin-left: 5px;
	background: url(../images/bussdeal/policy.png) no-repeat 0px -20px;
}

.buss_area .workarea .title_area .help .policy:hover {
	color: #626262;
	background-position: 0px 0px;
}

.buss_area .workarea .form_button_area {
	text-align: center;
	margin: 20px 0px;
}

/* 按钮 */
/* 操作类（下一步、上一步、查看、修改、查询） */
.button3 {
	width: auto;
	height: 30px;
	background-color: #9370db;
	color: #ffffff;
	font-family: "微软雅黑";
	font-size: 12px;
	letter-spacing: 3px;
	border: 0px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	cursor: pointer;
	padding: 0px 10px 0px 13px;
	margin: 5px 10px;
	overflow: visible;
}

.button3:hover {
	background-color: #9932cc;
}


