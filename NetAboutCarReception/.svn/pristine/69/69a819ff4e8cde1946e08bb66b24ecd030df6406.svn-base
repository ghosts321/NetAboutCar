package com.nasoft.SSM.domain;

import java.io.Serializable;

public class drvDisAppeal implements Serializable{

	private int id;//申诉处理id
	private int drv_complain_info_id;//投诉信息id
	private String disappeal_idea;//申诉处理意见
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}

	public int getDrv_complain_info_id() {
		return drv_complain_info_id;
	}
	public void setDrv_complain_info_id(int drv_complain_info_id) {
		this.drv_complain_info_id = drv_complain_info_id;
	}
	public String getDisappeal_idea() {
		return disappeal_idea;
	}
	public void setDisappeal_idea(String disappeal_idea) {
		this.disappeal_idea = disappeal_idea;
	}
		
}
